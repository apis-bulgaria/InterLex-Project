            var areaCol;
            if (result.Items[i].Details.Cases != null) {
                areaCol = $('<td style="width: 10%"><img src="/Content/Images/icon-cases-small.png" title="@Resources.UI_Cases" style="margin-right: 5px;" /><img src="/Content/Images/icon-law-small.png" style="opacity: 0.4;" /></td>');
            }
            else if (result.Items[i].Details.Law != null) {
                areaCol = $('<td style="width: 10%"><img src="/Content/Images/icon-cases-small.png" title="@Resources.UI_Cases" style="opacity: 0.4; margin-right: 5px;" /><img src="/Content/Images/icon-law-small.png" title="@Resources.UI_Legislation" /></td>');
            }
            else if (result.Items[i].Details.Finances != null) {
                areaCol = $('<td style="width: 10%"><img src="/Content/Images/icon-finances-doc-small.png" style="margin-right: 1px;" /></td>');
            }
            else {
                @if (int.Parse(Session["SelectedProductId"].ToString()) == 1)
                {
                    @: areaCol = $('<td style="width: 10%"><img src="/Content/Images/icon-cases-small.png" title="@Resources.UI_Cases" style="margin-right: 5px;" /><img src="/Content/Images/icon-law-small.png" title="@Resources.UI_Legislation" class="" /></td>');
                }
                else if (int.Parse(Session["SelectedProductId"].ToString()) == 2)
                {
                    @: areaCol = $('<td style="width: 10%"><img src="/Content/Images/icon-finances-doc-small.png" style="opacity: 0.4; margin-right: 1px;" /></td>');
                }
            }

            var dateCol = $('<td style="width: 15%">' + result.Items[i].DateAsString + '</td>');
            var loadSearchCol = $('<td style="width: 10%" class="my-searches-button-control"><a href="#" title="@Resources.Hint_LoadSearch" class="load-search" data-viewid="' + result.Items[i].Id + '" data-globalid="' + result.Items[i].Details.GlobalId + '"><span class="fa fa-pencil my-search-image"></span></a></td>');
            var doSearchCol = $('<td style="width: 10%" class="my-searches-button-control"><a href="#" title="@Resources.Hint_DoSearch" class="do-search" data-viewid="' + result.Items[i].Id + '" data-globalid="' + result.Items[i].Details.GlobalId + '"><span class="fa fa-search my-search-image"></span></a></td>');
            var delSearchCol = $('<td style="width: 10%" class="my-searches-button-control"><a href="#" title="@Resources.Hint_DelSearch" class="delete-search" data-delid="' + result.Items[i].Id + '"><span class="fa fa-times my-search-image"></span></a></td>');
            var row = $('<tr></tr>');

            row.append(textCol);
            row.append(areaCol);
            row.append(dateCol);
            row.append(loadSearchCol);
            row.append(doSearchCol);
            row.append(delSearchCol);

            documentFragment.append(row);
        }

        searchesListBody.append(documentFragment);

        $('.delete-search').click(deleteClick);
        $('.load-search').click(loadClick);
        $('.do-search').click(doClick);
    }

    $(window).resize(function () {
        if (getDeviceWidth() <= 600) {
            $('#my-searches-search-type-label').text('@Resources.UI_Type');
        }
        else {
            $('#my-searches-search-type-label').text('@Resources.UI_SearchType');
        }
    });
</script>