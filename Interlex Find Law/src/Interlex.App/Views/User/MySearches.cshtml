@model Interlex.BusinessLayer.Models.UserSearches
@{
    ViewBag.Title = "MySearches";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@using Interlex.App.Resources

<div class="container-fluid main-content">
    <div>
        <div class="filters-container">
            <div id="pAvaiableFilters">
                <div class="b-medium b-orange f-white f-bold" style="border-radius:3px;">@Resources.UI_Filters@*<span class="chevron-white-down-on-orange right"></span>*@</div>
                <div id="pAvaiableFiltersBody">
                    <div class="filter"><b> @Resources.UI_Period:</b></div>
                    <ul class="filter-ul-holder">
                        <li data-period="0" class="f-black list-group-item-info-period f-0-8 not-classifier"><a href="">@Resources.UI_All</a></li>
                        <li data-period="1" class="f-0-8 not-classifier"><a href="">@Resources.UI_Today</a></li>
                        <li data-period="2" class="f-0-8 not-classifier"><a href="">@Resources.UI_Yesterday</a></li>
                        <li data-period="3" class="f-0-8 not-classifier"><a href="">@Resources.UI_LastWeek</a></li>
                        <li data-period="4" class="f-0-8 not-classifier"><a href="">@Resources.UI_LastMonth</a></li>
                    </ul>
                    <div class="filter"><b> @Resources.UI_Type:</b></div>
                    <ul class="filter-ul-holder">
                        <li data-type="All" class="f-black list-group-item-info-type not-classifier"><a href="">@Resources.UI_All</a></li>
                        <li data-type="Simple" class="not-classifier"><a href="">@Resources.UI_Full</a></li>
                        @if (int.Parse(Session["SelectedProductId"].ToString()) == 1)
                        {
                            <li data-type="Law" class="not-classifier"><a href="">@Resources.UI_Legislation</a></li>
                            <li data-type="Cases" class="not-classifier"><a href="">@Resources.UI_Cases</a></li>
                        }
                        else if (int.Parse(Session["SelectedProductId"].ToString()) == 2)
                        {
                            <li data-type="FinancesAdvanced" class="not-classifier"><a href="">@Resources.UI_FinancesAdvanced</a></li>
                        }
                    </ul>
                </div>
            </div>
        </div>

        <div id="my-searches-container">
            <span class="f-blue inline-block margin-left-20 margin-top-15 f-bold page-main-title">@Resources.UI_MySearches_Upper</span>
            <div class="my-searches-table-container">
                <table class="" id="my-searches-table">
                    <thead>
                        <tr>
                            <th style="width: 45%">@Resources.UI_Search</th>
                            <th style="width: 10%" id="my-searches-search-type-label">
                                @(Request.Browser.IsMobileDevice ? Resources.UI_Type : Resources.UI_SearchType)
                            </th>
                            <th style="width: 15%">@Resources.UI_Date</th>
                            <th style="width: 10%"></th>
                            <th colspan="2" style="text-align: right" class="f-0-8"><a href="#" id="delete-all-searches-link"><span class="fa fa-trash-o"></span> @Resources.UI_DeleteAllSearches</a></th>
                            @*<th style="width: 10%;" class="f-0-8"></th>*@
                        </tr>
                    </thead>
                    <tbody id="searches-list-body">
                        @foreach (var search in Model.Items)
                        {
                            <tr class="text-primary">
                                <td style="width: 45%">
                                    @if (search.Text == "" || search.Text == String.Empty || String.IsNullOrEmpty(@search.Text))
                                    {
                                        <p><strong class="f-orange">@Resources.UI_NoSearchText</strong></p>
                                    }
                                    else
                                    {
                                        <p><strong>@Resources.UI_Text: </strong>@search.Text</p>
                                    }

                                    @if (search.Details.MultiDict != null && !String.IsNullOrEmpty(search.Details.MultiDict.Text))
                                    {
                                        <p>
                                            <strong class="f-blue"><span class="fa fa-book"></span> @Resources.UI_MultilingualDictionary: </strong>
                                            @search.Details.MultiDict.Text
                                        </p>
                                    }

                                    @if (search.Details.Cases != null)
                                    {
                                        if (search.Details.Cases.OnlyInTitles == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_SearchOnlyInTitles</p>
                                        }

                                        if (search.Details.Cases.TranslateSearchText == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_TranslateSearchText</p>
                                        }

                                        <p>
                                            <strong>@Resources.UI_CaseLawType: </strong>
                                            @switch (search.Details.Cases.CaseLawType)
                                            {
                                                case Interlex.BusinessLayer.Enums.CaseLawType.All:@Resources.UI_JS_AllCaseLaw; break;
                                            case Interlex.BusinessLayer.Enums.CaseLawType.EU: @Resources.UI_JS_EUCaseLaw; break;
                                        case Interlex.BusinessLayer.Enums.CaseLawType.National: @Resources.UI_JS_NationalCaseLaw; break;
                                    case Interlex.BusinessLayer.Enums.CaseLawType.ECHR: @Resources.UI_JS_ECHRCaseLaw; break;
                                default: @Resources.UI_JS_AllCaseLaw; break;
                        }
                                        </p>

                                        @* Old *@
                                        if (!String.IsNullOrEmpty(search.Details.Cases.MultiDictionaryText))
                                        {
                                            <p>
                                                <strong class="f-blue"><span class="fa fa-book"></span> @Resources.UI_MultilingualDictionary: </strong>
                                                @search.Details.Cases.MultiDictionaryText
                                            </p>
                                        }

                                        @* New *@
                                        if (search.Details.Cases.MultiDict != null && !String.IsNullOrEmpty(search.Details.Cases.MultiDict.Text))
                                        {
                                            <p>
                                                <strong class="f-blue"><span class="fa fa-book"></span> @Resources.UI_MultilingualDictionary: </strong>
                                                @search.Details.Cases.MultiDict.Text
                                            </p>
                                        }

                                        if (String.IsNullOrEmpty(search.Details.Cases.CaseNumber) == false)
                                        {
                                            <p>
                                                <strong>@Resources.UI_CaseNumber: </strong>
                                                @search.Details.Cases.CaseNumber
                                            </p>
                                        }

                                        if (String.IsNullOrEmpty(search.Details.Cases.Year) == false)
                                        {
                                            <p>
                                                <strong>@Resources.UI_Year: </strong>
                                                @search.Details.Cases.Year
                                            </p>
                                        }

                                        if (search.Details.Cases.Parties != null)
                                        {
                                            <p><strong>@Resources.UI_Parties: </strong>@search.Details.Cases.Parties</p>
                                        }

                                        if (search.Details.Cases.NatID_ECLI != null)
                                        {
                                            <p><strong>@Resources.UI_NatIdOrEcli: </strong>@search.Details.Cases.NatID_ECLI</p>
                                        }

                                        if (!String.IsNullOrEmpty(search.Details.Cases.Applicant))
                                        {
                                            <p>
                                                <strong>@Resources.UI_Cases_Applicant: </strong>@search.Details.Cases.Applicant
                                            </p>
                                        }

                                        if (!String.IsNullOrEmpty(search.Details.Cases.ApplicationNumber))
                                        {
                                            <p>
                                                <strong>@Resources.UI_Cases_ApplicationNumber: </strong>@search.Details.Cases.ApplicationNumber
                                            </p>
                                        }

                                        if (String.IsNullOrEmpty(search.Details.Cases.EnactmentText) == false)
                                        {
                                            <p>
                                                <strong>@Resources.UI_Enactment: </strong>
                                                @search.Details.Cases.EnactmentText
                                            </p>
                                        }

                                        if (String.IsNullOrEmpty(search.Details.Cases.ProvisionText) == false)
                                        {
                                            <p>
                                                <strong>@Resources.UI_Provision: </strong>
                                                @search.Details.Cases.ProvisionText
                                            </p>
                                        }

                                        @*if (search.Details.Cases.AdvocateGeneral.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_GeneralLawyer: </strong>@String.Join(", ", search.Details.Cases.AdvocateGeneral.TitlePaths)</p>
                                            }*@

                                        if (search.Details.Cases.CourtsFolders.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Court: </strong>@String.Join(", ", search.Details.Cases.CourtsFolders.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.DateFrom != default(DateTime) || search.Details.Cases.DateTo != default(DateTime))
                                        {
                                            <p>
                                                @if (search.Details.Cases.DateFrom != default(DateTime))
                                                {
                                                    <strong>@Resources.UI_From_Lower: </strong>@search.Details.Cases.DateFrom.ToShortDateString()


                                                }

                                                @if (search.Details.Cases.DateTo != default(DateTime))
                                                {
                                                    <strong>@Resources.UI_To_Lower: </strong>@search.Details.Cases.DateTo.ToShortDateString()
                                                }
                                            </p>
                                        }

                                        if (search.Details.Cases.DocumentTypes.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_DocumentType: </strong>@String.Join(", ", search.Details.Cases.DocumentTypes.TitlePaths)</p>
                                        }

                                        @*if (search.Details.Cases.DirectoryCaseLaw.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_DirectoryCaseLaw: </strong>@String.Join(", ", search.Details.Cases.DirectoryCaseLaw.TitlePaths)</p>
                                            }*@

                                        @*if (search.Details.Cases.EuroCases.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_EUCases: </strong>@String.Join(", ", search.Details.Cases.EuroCases.TitlePaths)</p>
                                            }*@

                                        @*if (search.Details.Cases.Eurovoc.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_Eurovoc: </strong>@String.Join(", ", search.Details.Cases.Eurovoc.TitlePaths)</p>
                                            }*@

                                        @*if (search.Details.Cases.JudgeRapporteur.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_JudgeRapporteur: </strong>@String.Join(", ", search.Details.Cases.JudgeRapporteur.TitlePaths)</p>
                                            }*@

                                        @*if (search.Details.Cases.ProcedureType.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_ProcedureType: </strong>@String.Join(", ", search.Details.Cases.ProcedureType.TitlePaths)</p>
                                            }*@

                                        if (search.Details.Cases.Syllabus.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Syllabus: </strong>@String.Join(", ", search.Details.Cases.Syllabus.TitlePaths)</p>
                                        }

                                        @*if (search.Details.Cases.SubjectMatter.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_Subjectmatter: </strong>@String.Join(", ", search.Details.Cases.SubjectMatter.TitlePaths)</p>
                                            }*@

                                        if (search.Details.Cases.Courts != null && search.Details.Cases.Courts.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_Courts: </strong>@String.Join(", ", search.Details.Cases.Courts.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.Hudoc != null && search.Details.Cases.Hudoc.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_Hudoc: </strong>@String.Join(", ", search.Details.Cases.Hudoc.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.HudocImportance != null && search.Details.Cases.HudocImportance.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_HudocImportance: </strong>@String.Join(", ", search.Details.Cases.HudocImportance.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.HudocArticleViolation != null && search.Details.Cases.HudocArticleViolation.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_HudocArticleViolation: </strong>@String.Join(", ", search.Details.Cases.HudocArticleViolation.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.HudocApplicability != null && search.Details.Cases.HudocApplicability.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_HudocApplicability: </strong>@String.Join(", ", search.Details.Cases.HudocApplicability.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.States != null && search.Details.Cases.States.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_States: </strong>@String.Join(", ", search.Details.Cases.States.TitlePaths)</p>
                                        }

                                        if (!String.IsNullOrEmpty(search.Details.Cases.ReferedActTitle))
                                        {
                                            <p><strong>@Resources.UI_Cases_ReferedAct: </strong>@search.Details.Cases.ReferedActTitle</p>
                                        }

                                        if (search.Details.Cases.HudocArticles != null && search.Details.Cases.HudocArticles.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_HudocArticles: </strong>@String.Join(", ", search.Details.Cases.HudocArticles.TitlePaths)</p>
                                        }

                                        if (search.Details.Cases.RulesOfTheCourt != null && search.Details.Cases.RulesOfTheCourt.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Cases_RulesOfTheCourt: </strong>@String.Join(", ", search.Details.Cases.RulesOfTheCourt.TitlePaths)</p>
                                        }
                                    }

                                    @if (search.Details.Law != null)
                                    {
                                        @* <p>
                                                <strong>@Resources.UI_SearchArea: </strong>
                                                @switch (search.Details.Law.LegislationType)
                                                {
                                                    case Interlex.BusinessLayer.Enums.LegislationType.EU: @Resources.UI_EuLegislation; break;
                                                    case Interlex.BusinessLayer.Enums.LegislationType.National: @Resources.UI_NationalLegislation; break;
                                                    default: @Resources.UI_EuLegislation; break;
                                                }
                                            </p>
                                        *@
                                        if (search.Details.Law.OnlyInTitles == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_SearchOnlyInTitles</p>
                                        }

                                        if (search.Details.Law.TranslateSearchText == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_TranslateSearchText</p>
                                        }

                                        if (search.Details.Law.OnlyInActualActs == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_SearchOnlyInActualActs</p>
                                        }

                                        if (search.Details.Law.OnlyInBasicActs == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_SearchOnlyInBasicActs</p>
                                        }

                                        if (search.Details.Law.MultiDict != null && !String.IsNullOrEmpty(search.Details.Law.MultiDict.Text))
                                        {
                                            <p>
                                                <strong class="f-blue"><span class="fa fa-book"></span> @Resources.UI_MultilingualDictionary: </strong>
                                                @search.Details.Law.MultiDict.Text
                                            </p>
                                        }

                                        if (search.Details.Law.NatID_ECLI != null)
                                        {
                                            <p><strong>@Resources.UI_NatIdOrEcli: </strong>@search.Details.Law.NatID_ECLI</p>
                                        }

                                        if (search.Details.Law.DateFrom != default(DateTime) || search.Details.Law.DateTo != default(DateTime))
                                        {
                                            <p>
                                                <strong>@Resources.UI_DateType: </strong>
                                                @{
                                                    var resourceString = "UI_LawDateType_" + (int)search.Details.Law.LawDateType;
                                                    var resource = Res.GetResource(resourceString);
                                                }
                                            </p>

                                            <p>
                                                @if (search.Details.Law.DateFrom != default(DateTime))
                                                {
                                                    <strong>@Resources.UI_From_Lower: </strong>@search.Details.Law.DateFrom.ToShortDateString()
                                                }

                                                @if (search.Details.Law.DateTo != default(DateTime))
                                                {
                                                    <strong>@Resources.UI_To_Lower: </strong>@search.Details.Law.DateTo.ToShortDateString()

                                                }
                                            </p>
                                        }

                                        if (String.IsNullOrEmpty(search.Details.Law.DocNumber) == false)
                                        {
                                            <p>
                                                <strong>
                                                    @Resources.UI_Number:
                                                </strong>
                                                @search.Details.Law.DocNumber
                                            </p>
                                        }

                                        if (String.IsNullOrEmpty(search.Details.Law.Year) == false)
                                        {
                                            <p>
                                                <strong>
                                                    @Resources.UI_Year:
                                                </strong>
                                                @search.Details.Law.Year
                                            </p>
                                        }

                                        if (search.Details.Law.Number != null || search.Details.Law.PageNumber != null || search.Details.Law.OJYear != null)
                                        {
                                            <p>
                                                <strong>
                                                    @(search.Details.Law.LegislationType == Interlex.BusinessLayer.Enums.LegislationType.EU ? Resources.UI_OJSeries + ":" : Resources.UI_Publication + ":")
                                                </strong>
                                                @if (search.Details.Law.LegislationType == Interlex.BusinessLayer.Enums.LegislationType.EU && search.Details.Law.OJSeries != ApisLucene.Classes.Common.OJTypeEnum.DEFAULT)
                                                {
                                                    @search.Details.Law.OJSeries.ToString()
                                                }
                                                else if (search.Details.Law.LegislationType == Interlex.BusinessLayer.Enums.LegislationType.EU)
                                                {
                                                    @Resources.UI_AllOJSeries
                                                }
                                                else
                                                {
                                                    <span>Publication source</span>
                                                }

                                                @if (search.Details.Law.Number != null)
                                                {
                                                    <strong>@Resources.UI_Number: </strong>@search.Details.Law.Number
                                                }

                                                @if (search.Details.Law.PageNumber != null)
                                                {
                                                    <strong>@Resources.UI_PageNumber: </strong>@search.Details.Law.PageNumber
                                                }

                                                @if (search.Details.Law.OJYear != null)
                                                {
                                                    <strong>@Resources.UI_Year: </strong>@search.Details.Law.OJYear


                                                }
                                            </p>
                                        }

                                        if (search.Details.Law.Jurisdictions.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Jurisdiction: </strong>@String.Join(", ", search.Details.Law.Jurisdictions.TitlePaths)</p>
                                        }

                                        if (search.Details.Law.DocumentTypes.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_DocumentType: </strong>@String.Join(", ", search.Details.Law.DocumentTypes.TitlePaths)</p>
                                        }

                                        @*if (search.Details.Law.DirectoryLegislation.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_DirectoryLegislation: </strong>@String.Join(", ", search.Details.Law.DirectoryLegislation.TitlePaths)</p>
                                            }

                                            if (search.Details.Law.Eurovoc.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_Eurovoc: </strong>@String.Join(", ", search.Details.Law.Eurovoc.TitlePaths)</p>
                                            }*@

                                        if (search.Details.Law.ActJurisdictions.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Law_ActJurisdictions: </strong>@String.Join(", ", search.Details.Law.ActJurisdictions.TitlePaths)</p>
                                        }

                                        if (search.Details.Law.Syllabus.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Syllabus: </strong>@String.Join(", ", search.Details.Law.Syllabus.TitlePaths)</p>
                                        }

                                        @*if (search.Details.Law.SubjectMatter.TitlePaths.Length != 0)
                                            {
                                                <p><strong>@Resources.UI_Subjectmatter: </strong>@String.Join(", ", search.Details.Law.SubjectMatter.TitlePaths)</p>
                                                }
                                            }*@
                                    }
                                    @if (search.Details.Finances != null)
                                    {
                                        if (search.Details.Finances.OnlyInTitles == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_Finances_SearchInTitles</p>
                                        }

                                        if (search.Details.Finances.Keywords == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_Finances_SearchInKeywords</p>
                                        }

                                        if (search.Details.Finances.SearchInSummaries == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_Finances_SearchInSummaries</p>
                                        }

                                        if (search.Details.Finances.DocTypeStandarts)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_JS_FinStandarts</p>
                                        }

                                        if (search.Details.Finances.DocTypeDirectives == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_JS_Directives</p>
                                        }

                                        if (search.Details.Finances.DocTypeReglaments == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_JS_Reglaments</p>
                                        }

                                        if (search.Details.Finances.DocTypeEuCaseLaw == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_EUCaseLaw</p>
                                        }

                                        if (search.Details.Finances.DocTypeNationalCaseLaw == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_NationalCaseLaw</p>
                                        }

                                        if (search.Details.Finances.DocTypeSummaries == true)
                                        {
                                            <p class="f-blue"><span class="fa fa-check"></span> @Resources.UI_Fins_DocTypeSummaries</p>
                                        }

                                        <p>
                                            @if (search.Details.Finances.DateFrom != default(DateTime))
                                            {
                                                <strong>@Resources.UI_From_Lower: </strong>@search.Details.Finances.DateFrom.ToShortDateString()

                                            }

                                            @if (search.Details.Finances.DateTo != default(DateTime))
                                            {
                                                <strong>@Resources.UI_To_Lower: </strong>@search.Details.Finances.DateTo.ToShortDateString()

                                            }
                                        </p>
                                        if (search.Details.Finances.EuroFinance.TitlePaths.Length != 0)
                                        {
                                            <p><strong>@Resources.UI_Finances_EuroFinance: </strong>@String.Join(", ", search.Details.Finances.EuroFinance.TitlePaths)</p>
                                        }
                                    }
                                </td>
                                <td style="width: 10%">
                                    @if (search.Details.Law != null)
                                    {
                                        <img src="~/Content/Images/libra-doc.png" title="@Resources.UI_Cases" style="opacity: 0.4; margin-right: 1px;" />
                                        <img src="~/Content/Images/column1.png" title="@Resources.UI_Legislation" />
                                    }
                                    else if (search.Details.Cases != null)
                                    {
                                        <img src="~/Content/Images/libra-doc.png" title="@Resources.UI_Cases" style="margin-right: 1px" />
                                        <img src="~/Content/Images/column1.png" title="@Resources.UI_Legislation" style="opacity: 0.4;" />
                                    }
                                    else if (search.Details.Finances != null)
                                    {
                                        <img src="~/Content/Images/icon-finances-doc-small.png" style="margin-right: 1px;" />
                                    }
                                    else if (search.Details.Cases == null && search.Details.Law == null && search.Details.Finances == null)
                                    {
                                        if (int.Parse(Session["SelectedProductId"].ToString()) == 1)
                                        {
                                            <img src="~/Content/Images/libra-doc.png" title="@Resources.UI_Cases" style="margin-right: 1px" />
                                            <img src="~/Content/Images/column1.png" title="@Resources.UI_Legislation" />
                                        }
                                        else if (int.Parse(Session["SelectedProductId"].ToString()) == 2)
                                        {
                                            <img src="~/Content/Images/icon-finances-doc-small.png" style="opacity: 0.4; margin-right: 1px;" />
                                        }
                                    }
                                </td>
                                <td style="width: 15%">@search.Date</td>
                                <td style="width: 10%" class="my-searches-button-control">
                                    <a href="#" class="load-search" data-viewid="@search.Id" data-globalid="@search.Details.GlobalId" title="@Resources.Hint_LoadSearch">
                                        <span class="fa fa-pencil my-search-image"></span>
                                    </a>
                                </td>
                                <td style="width: 10%" class="my-searches-button-control">
                                    <a href="#" class="do-search" data-viewid="@search.Id" data-globalid="@search.Details.GlobalId" title="@Resources.Hint_DoSearch">
                                        <span class="fa fa-search my-search-image"></span>
                                    </a>
                                </td>
                                <td style="width: 10%" class="my-searches-button-control">
                                    <a href="#" class="delete-search" data-delid="@search.Id" title="@Resources.Hint_DelSearch">
                                        <span class="fa fa-times my-search-image"></span>
